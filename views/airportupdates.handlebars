<section id="flight" class="container">
  <h1>All Airports</h1>


  {{#each AirportUpdates}}
  <div class="flight">
    <h3>Airport Updates logs:</h3>
    <p>id: {{id}}</p>
    <ul>
      <li>Country: {{country}}</li>
      <li>Previous Airport: {{oldAirport}}</li>
      <li>New Airport: {{newAirport}}</li>
    </ul>
  </div>
  {{else}}
  <p>No data available</p>
  {{/each}}
</section>